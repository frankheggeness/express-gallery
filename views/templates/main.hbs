  <title>_Architekt</title>
  <link rel="stylesheet" type="text/css" href="css/styles.css/styles.css">
  {{>fonts}}
</head>

<body>
  {{>header}}
  <div id="contentContainer">
    <div id="bigContentBox">
      <div class="bigPic">
        <img class="bigImage"
          src="https://d14fqx6aetz9ka.cloudfront.net/wp-content/uploads/2019/02/08102107/Koa_Rothman_aframe_noyle__ZN14772-1.jpg"
