      <div id="stacked1" class="stackedBox">
        <div class="stackedPic">
          <img class="smallPic"
            src="https://www.surfholidays.com/assets/images/blog/2015-12-29-Kelly_Slater_Profile_Tube_ride.jpg" alt="">
        </div>
        <div class="stackedText">this is box 1</div>
      </div>
      <div id="stacked2" class="stackedBox">
        <div class="stackedPic"></div>
        <div class="stackedText">this is box 2</div>
      </div>
      <div id="stacked3" class="stackedBox">
        <div class="stackedPic">

        </div>
        <div class="stackedText">this is box 3</div>
      </div>
    </div>
  </div>
